{
  "name": "server",
  "version": "1.0.0",
  "description": "server setup",
  "type": "module",
  "scripts": {
    "start": "npm run prod",
    "dev": "npx babel-node ./src/basic-server.js",
    "dev-server": "nodemon --exec babel-node ./src/basic-server.js",
    "build": "npm-run-all clean transpile",
    "clean": "rimraf dist",
    "transpile": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production node ./dist/basic-server.js",
    "jwt-server": "nodemon --require dotenv/config --exec babel-node ./src/jwt-server.js",
    "server:prod:jwt": "node ./dist/jwt-server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "AP",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "got": "^11.1.2",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.9.11",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "rimraf": "^3.0.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.14.0",
    "nodemon": "^2.0.3"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  }
}
