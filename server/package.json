{
  "name": "server",
  "version": "1.0.0",
  "description": "server setup",
  "main": "basic-server.js",
  "scripts": {
    "start": "npm run prod",
    "dev": "npx babel-node ./src/basic-server.js",
    "dev-server": "nodemon --exec babel-node ./src/basic-server.js",
    "server:prod": "node ./dist/basic-server.js",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "AP",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^3.22.0",
    "mongoose": "^5.9.11",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "nodemon": "^2.0.3"
  },
  "engines": {
    "node": ">= 12.14.1"
  }
}
